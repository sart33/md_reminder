<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_background_common"
    tools:context=".presentation.reminder.fragment.ReminderListAtDate">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/header_layout_height"
        app:cardBackgroundColor="@color/first_background_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/first_background_color">

            <ImageView
                    android:id="@+id/ivMainMenu"
                    android:layout_width="@dimen/menu_icon_width"
                    android:layout_height="@dimen/menu_height"
                    android:layout_marginStart="16dp"
                    android:contentDescription="@string/open_menu"
                    android:paddingTop="12dp"
                    android:paddingBottom="12dp"
                    android:src="@drawable/menu_1"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvContactsList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/comfortaa_regular"
                android:text="@string/contacts"
                android:textColor="@color/color_primary_text"
                android:textSize="@dimen/name_current_text_size"
                app:autoSizeMaxTextSize="@dimen/name_max_text_size"
                app:autoSizeMinTextSize="@dimen/name_min_text_size"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/ivMainMenu"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="675dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView6">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clContactOne"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="@dimen/cv_width"
                    android:layout_height="100dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/textview_border"
                    android:padding="10dp"
                    app:cardBackgroundColor="@color/background_none"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/tvNameOne"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/comfortaa_regular"
                        android:padding="@dimen/padding_action"
                        android:text="Mobile developer"
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvPhoneNumberOne" />

                    <TextView
                        android:id="@+id/tvPhoneNumberOne"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:layout_marginStart="16dp"
                        android:layout_marginTop="6dp"
                        android:fontFamily="@font/comfortaa_regular"
                        android:padding="@dimen/padding_action"

                        android:text="Gleb Ivanchik"
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clContactTwo"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:visibility="visible"
                tools:layout_editor_absoluteY="380dp">

                <ImageView
                        android:id="@+id/imageViewTwo"
                        android:layout_width="@dimen/menu_icon_width"
                        android:layout_height="@dimen/menu_icon_height"
                        android:layout_marginStart="24dp"
                        android:contentDescription="@string/email"
                        android:padding="7dp"
                        android:src="@drawable/cnt_mailchapter"
                        android:visibility="visible"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout2"
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/background_none"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/tvDeveloperMail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="email"
                        android:fontFamily="@font/comfortaa_regular"
                        android:paddingTop="15dp"
                        android:paddingBottom="15dp"
                        android:text="glebivanchik@gmail.com"
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="@dimen/reminder_contact_name"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clContactFour"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginBottom="@dimen/margin_full_action"
                android:visibility="visible">


                <ImageView
                        android:id="@+id/imageViewFour"
                        android:layout_width="@dimen/menu_icon_width"
                        android:layout_height="@dimen/menu_icon_height"
                        android:layout_marginStart="24dp"
                        android:contentDescription="@string/site"
                        android:padding="7dp"
                        android:src="@drawable/site"
                        android:visibility="visible"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/background_none"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/tvNameFour"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="web"
                        android:autoText="false"
                        android:fontFamily="@font/comfortaa_regular"
                        android:paddingTop="15dp"
                        android:paddingBottom="15dp"
                        android:text="mobiledev.solutions"
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="@dimen/reminder_contact_name"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clContactFive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="visible">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="@dimen/cv_width"
                    android:layout_height="100dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="5dp"
                    android:background="@drawable/textview_border"
                    android:padding="10dp"
                    app:cardBackgroundColor="@color/background_none"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0">

                    <TextView
                        android:id="@+id/tvNameFive"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/comfortaa_regular"
                        android:lineHeight="24dp"
                        android:padding="@dimen/padding_action"
                        android:text="Designer"
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/tvPhoneNumberFive" />

                    <TextView
                        android:id="@+id/tvPhoneNumberFive"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:layout_marginStart="16dp"
                        android:layout_marginTop="6dp"

                        android:fontFamily="@font/comfortaa_regular"
                        android:padding="@dimen/padding_action"
                        android:text="Vladimit Maximenko "
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="20sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/clContactSix"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:visibility="visible"
                tools:layout_editor_absoluteY="380dp">

                <ImageView
                        android:id="@+id/imageViewSix"
                        android:layout_width="@dimen/menu_width"
                        android:layout_height="@dimen/menu_icon_height"
                        android:layout_marginStart="24dp"
                        android:contentDescription="@string/email"
                        android:padding="7dp"
                        android:src="@drawable/cnt_mailchapter"
                        android:visibility="visible"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout7"
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    app:cardBackgroundColor="@color/background_none"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <TextView
                        android:id="@+id/tvNameSeven"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:autoLink="email"
                        android:fontFamily="@font/comfortaa_regular"
                        android:paddingTop="15dp"
                        android:paddingBottom="15dp"
                        android:text="vvm-2019@proton.me "
                        android:textColor="@color/color_primary_text"
                        android:textDirection="locale"
                        android:textSize="@dimen/reminder_contact_name"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>